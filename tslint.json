{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-over-type-literal": false,
    "object-literal-sort-keys": false,
    "variable-name": {
      "options": [
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case"
      ]
    },
    "no-unused-variable": true,
    "no-shadowed-variable": false
  }
}
